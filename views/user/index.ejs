<div class="container">
<h3>List of Users</h3>
	<table class="table table-striped table-condensed">
		<tr class="success">
			<th></th>
			<th>Username</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>E-mail</th>
			<th>URL</th>
			<th>Role</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
		<% users.forEach(function(user){ %>
		<tr>
			<% if(user.online){ %>
			<td><img src="/images/icon-online.png" alt="online"></td>
			<% }else{ %>
			<td><img src="/images/icon-offline.png" alt="offline"></td>
			<% } %>
			<td><%= user.username %></td>
			<td><%= user.firstname %></td>
			<td><%= user.lastname %></td>
			<td><%= user.email %></td>
			<td><%= user.url %></td>
			<% if (user.admin){ %>
			<td><img src="/images/admin.png" alt="admin"></td>
			<% }else{ %>
			<td><img src="/images/pawn.png" alt="pawn"></td>
			<% } %>
			<td><a href="/user/readF/<%= user.username%>"  class="btn btn-primary">show</a></td>
			<td><a href="/user/updateF/<%= user.username%>"  class="btn btn-success">edit</a></td>
			<td>
				<form action="/user/delete/<%= user.username%>" method="POST">
					<input type="submit" value="delete" class="btn btn-danger btnDeleteUser">
					<input type="hidden" name="_method" value="delete">
					<input type="hidden" name="_csrf" value="<%= _csrf %>" />
				</form>
			</td>
		</tr>
		<% }); %>
	</table>
</div>